{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "paths": {
      "@angular/*": ["node_modules/@angular/*"],
      "@data-viz/area-chart": [
        "libs/data-viz/shared/components/area-chart/src/index.ts"
      ],
      "@data-viz/bar-chart": [
        "libs/data-viz/shared/components/bar-chart/src/index.ts"
      ],
      "@data-viz/bubble-chart": [
        "libs/data-viz/shared/components/bubble-chart/src/index.ts"
      ],
      "@data-viz/donut-chart": [
        "libs/data-viz/shared/components/donut-chart/src/index.ts"
      ],
      "@data-viz/force-layout-chart": [
        "libs/data-viz/shared/components/force-layout-chart/src/index.ts"
      ],
      "@data-viz/grouped-bar-chart": [
        "libs/data-viz/shared/components/grouped-bar-chart/src/index.ts"
      ],
      "@data-viz/horizontal-bar-chart": [
        "libs/data-viz/shared/components/horizontal-bar-chart/src/index.ts"
      ],
      "@data-viz/line-chart": [
        "libs/data-viz/shared/components/line-chart/src/index.ts"
      ],
      "@data-viz/pie-chart": [
        "libs/data-viz/shared/components/pie-chart/src/index.ts"
      ],
      "@data-viz/scatter-chart": [
        "libs/data-viz/shared/components/scatter-chart/src/index.ts"
      ],
      "@data-viz/scatter-shape-chart": [
        "libs/data-viz/shared/components/scatter-shape-chart/src/index.ts"
      ],
      "@data-viz/services": ["libs/data-viz/shared/services/src/index.ts"],
      "@data-viz/stack-chart": [
        "libs/data-viz/shared/components/stack-chart/src/index.ts"
      ],
      "@data-viz/types": ["libs/data-viz/shared/types/src/index.ts"],
      "@portfolio/contact-us": [
        "libs/shared/components/contact-us/src/index.ts"
      ],
      "@portfolio/contact-us-api": [
        "libs/shared/services/feature-contact-us-api/src/index.ts"
      ],
      "@portfolio/feature-portfolio-lib": [
        "libs/portfolio/feature-portfolio-lib/src/index.ts"
      ],
      "feature-dataviz-lib": ["libs/data-viz/feature-dataviz-lib/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
